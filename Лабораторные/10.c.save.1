#include <stdio.h>
#include <stdlib.h>
#include <time.h>

const int N = 3;
void my_print(int ArrayLen, int * arr[])
{
	printf("Ваш массив:\n");
	for (int j = 0; j < ArrayLen; j++)
	{

			printf(" %d ", arr[j]);
	}
}

void CreateRandom(int ArrayLen, int *arr[])
{
	srand(time(NULL));
	for (int i = 0; i < ArrayLen; i++)
	{
			arr[i] = random() % 100;
	}
}

void CreatePrint(int ArrayLen, int arr[])
{
	int element;
	printf("Введите эллементы массива:\n");
	for (int i = 0; i < ArrayLen; i++)
	{
			scanf("%d", &element);
			arr[i][j] = element;
	}
}



int main()
{
	int way;
	int arr[N];
	printf("Выбирите способ заполнения матрицы:\n1 - Случайные числа\n2 - Ввод "
		   "чисел с клавиатуры\n");
	scanf("%d", &way);
	switch (way)
	{
	case 1:
		CreateRandom(N, arr);
		my_print(N, arr);
		break;
	case 2:
		CreatePrint(N, arr);
		my_print(N, arr);
		break;
	default:
		printf("Не верно выбран способ заполнения");
		break;
	}

}
