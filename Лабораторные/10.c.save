		{
			printf("[%d]", i);
			printf("{%d}", j);
		}
		printf("\n");
	}
}
int main() {
	int way;
	const int N = 3;
	int arr[N][N];
	printf("Выбирите способ заполнения матрицы:\n");
	printf("1 - Случайные числа\n");
	printf("2 - Ввод чисел с клавиатуры\n");
	scanf("%d", &way);
	if(way == 1){
		CreateRandom(N, arr);
	}
	else {
		if(way == 2){
			CreatePrint(N, arr);
		}
		else{
			printf("Не вверно выбран способ заполнения");
		}
	}
}
