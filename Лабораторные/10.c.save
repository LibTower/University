#include <stdlib.h>
#include <stdio.h>
#include <time.h>
int CreateRandom(int N, int arr[0][0])
{	srand(time(NULL));
	printf("Ваш массив:\n");
	for(int i = 0; i<N; i++){
		for(int j = 0; j<N; j++){
			arr[i][j] = random()%100;
			printf("%d ", arr[i][j]);
		}
		printf("\n");
	}
}
int CreatePrint(int N, int arr[0][0])
{	int element;
	printf("Введите эллементы массива:\n");
	for(int i = 0; i<N; i++)
	{
		for(int j = 0; j<N; j++)
		{
			scanf("%d", &element);
			arr[i][j] =  element;
